<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Address</title>
  </head>
  <body>
    <p><%=dataJsonContact[0].nama%></p>
    <%for(var i = 0; i < dataJSONAddresses.length; i++) {%>
    <p><%= dataJSONAddresses[i].street%></p>
    <p><%= dataJSONAddresses[i].city%></p>
    <p><%= dataJSONAddresses[i].zipcode%></p>
    <%}%>
    <form action="/contacts/addresses/<%= dataJsonContact[0].id%>" method="post">
      <p>
        <label for="">street</label>
        <input type="input" name="street">
      </p>
      <p>
        <label for="">city</label>
        <input type="input" name="city">
      </p>
      <p>
        <label for="">zipcode</label>
        <input type="input" name="zipcode">
      </p>
      <p>
        <input type="submit" value="submit">
      </p>
    </form>
  </body>
</html>
